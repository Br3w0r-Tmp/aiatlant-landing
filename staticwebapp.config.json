{
    "routes": [
      {
        "route": "/en/*",
        "serve": "/en/index.html",
        "statusCode": 200
      },
      {
        "route": "/ru/*",
        "serve": "/ru/index.html",
        "statusCode": 200
      },
      {
        "route": "/*",
        "serve": "/index.html",
        "statusCode": 200
      }
    ],
    "navigationFallback": {
      "rewrite": "index.html"
    }
  }