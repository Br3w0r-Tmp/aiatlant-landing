---
import content from '../../content/atlant.json';

const {menu, headerButtons} = content;

---
<header>
  <h2 class="logo">AI Atlant</h2>
  <nav class="navigation">
    {menu.map((item) => (
      <a href={item.href}>{item.title}</a>
    ))}
  </nav>
  <div class="actions">
    {headerButtons.map((item) => (
      <a href={item.href}>{item.label}</a>
    ))}
  </div>
</header>